file = {:Users/bwood/Library/Application Support/Mendeley Desktop/Downloaded/Vlek et al. - 2016 - Spontaneous Charge Carrier Localization in Extended One-Dimensional Systems.pdf:pdf},
issn = {10797114},
journal = {Physical Review Letters},
