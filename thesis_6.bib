@article{Bredas1985,
abstract = {Ab-initio Hartree-Fock and valence-effective-Hamiltonian calcns. were done on polyparaphenylene, polypyrrole, and polythiophene dimers, and polymer chains. A study was made, as a function of the torsion angle between consecutive rings, of electronic properties such as ionization potential, band gap and width of the highest occupied bands, and of the carbon-carbon bond length between rings. Many derivs. of these compds. have substituents that lead to an increase of the torsion angle between adjacent rings, as a result of steric interactions. On going from a coplanar to a perpendicular conformation, the ionization potential and bandgap increase, and the width of the highest occupied bands decreases. This makes it more difficult to ionize or reduce the polymer chains, and can result in achieving lower max. elec. conductivities on doping. Since the evolution of the electronic properties follows a cosine law (related to the decrease of the overlap between the p orbitals on adjacent rings), the modifications up to .apprx.40 Deg torsion angle are not very large. In all three polymers, the ionization potential for a 40 Deg torsion angle is about 0.4 eV larger than the coplanar-conformation value. Therefore, substituents that lead to torsion angles between consecutive rings smaller than 40 Deg are quite acceptable. For the substituted compds. the importance is discussed of the possibility of achieving a coplanar conformation upon doping, to permit high intrachain mobilities of charge carriers such as bipolarons. [on SciFinder (R)]},
archivePrefix = {arXiv},
arxivId = {arXiv:1011.1669v3},
author = {Br{\'{e}}das, J. L. and Street, G. B. and Th{\'{e}}mans, B. and Andr{\'{e}}, J. M.},
